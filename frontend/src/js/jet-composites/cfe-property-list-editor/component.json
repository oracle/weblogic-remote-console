{
  "name": "cfe-property-list-editor",
  "version": "1.0.0",
  "jetVersion": "^11.1.0",
  "displayName": "A user friendly, translatable name of the component.",
  "description": "A translatable high-level description for the component.",
  "dependencies": {
    "wrc-frontend-apis": "^1.0.0",
    "wrc-frontend-core": "^1.0.0",
    "wrc-frontend-integration": "^1.0.0",
    "wrc-frontend-microservices": "^1.0.0"
  },
  "properties": {
    "propertyListName": {
      "description": "Name of property list, which should be set to property name specified in PDJ.",
      "type": "string",
      "value": "Properties"
    },
    "nameHeaderLabel": {
      "description": "Header of the Name column",
      "type": "string"
    },
    "valueHeaderLabel": {
      "description": "Header of the Value column",
      "type": "string"
    },
    "propertiesString": {
      "description": "JSON string that represents the object with name and value pair. ",
      "type": "string",
      "value": ""
    },
    "readonly": {
      "description": "Indicates if this property list editor is read only, defaults to false.",
      "type": "boolean"
    },
    "resize": {
      "description": "",
      "type": "string",
      "value": "both"
    },
    "width": {
      "description": "",
      "type": "string",
      "value": "40.625rem"
    },
    "addButtonTooltip": {
      "description": "Tooltip for the add-row icon",
      "type": "string"
    },
    "deleteButtonTooltip": {
      "description": "Tooltip for the trash can icon",
      "type": "string"
    }
  },
  "methods": {
    "getPropertyListChangeResults": {
      "description": "Returns JS object for latest property list entries."
    },
    "updatePropertyListSnapshot": {
      "description": "Update snapshot of property list entries."
    }
  },
  "events": {
    "propertyAdded": {
      "description": "",
      "bubbles": true,
      "detail": {
        "displayName": "property",
        "description": "",
        "type": "object"
      }
    },
    "propertyChanged": {
      "description": "",
      "bubbles": true,
      "detail": {
        "displayName": "property",
        "description": "",
        "type": "object"
      }
    },
    "propertyDeleted": {
      "description": "",
      "bubbles": true,
      "detail": {
        "displayName": "property",
        "description": "",
        "type": "object"
      }
    }
  },
  "slots": {

  }
}
